{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/html"
          },
          {
            "key": "Content-Type",
            "value": "application/javascript",
            "source": "\\.(js|mjs)$"
          },
          {
            "key": "Content-Type",
            "value": "text/css",
            "source": "\\.(css)$"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }  